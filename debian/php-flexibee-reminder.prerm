#!/bin/bash
# rm script for php-flexibee-reminder-papermail

su - $WEBUSER -s /bin/bash -c 'COMPOSER_HOME="/var/lib/composer" composer -o remove listonoska/api -d /usr/lib/php-flexibee-reminder/'
