{   
    "name": "deb/flexibee-reminder",
    "description": "Reminder sender for FlexiBee written in PHP",
    "type": "project",
    "version": "0.3",
    "authors": [
        {   
            "name": "Vítězslav Dvořák",
            "email": "info@vitexsoftware.cz"
        }
    ],
    "config": {
        "vendor-dir": "/var/lib/flexibee-reminder"
    },
    "require": {
        "deb/flexibee": "*",
        "deb/ease-core": "*",
        "deb/flexibee-bricks": "*"
    },
    "autoload": {
        "psr-4": {
            "FlexiPeeHP\\Reminder\\": "./Reminder/"
        },
        
    },
    "repositories": [
        {   
            "type": "path",
            "url": "/usr/share/php/Ease"
        },
        {   
            "type": "path",
            "url": "/usr/share/php/EaseHtml/"
        },
        {   
            "type": "path",
            "url": "/usr/share/php/FlexiPeeHP"
        },
        {   
            "type": "path",
            "url": "/usr/share/php/EaseBricks/"
        },
        {   
            "type": "path",
            "url": "/usr/share/php/EaseTWB/"
        },
        {   
            "type": "path",
            "url": "/usr/share/php/FlexiPeeHPBricks"
        }
    ]

}
