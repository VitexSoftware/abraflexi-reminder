{
    "name": "vitexsoftware/abraflexi-reminder",
    "description": "Reminder sender for AbraFlexi written in PHP",
    "authors": [
        {
            "name": "Vítězslav Dvořák",
            "email": "info@vitexsoftware.cz"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "vitexsoftware/ease-core": "^1",
        "spoje.net/flexibee": "^1",
        "vitexsoftware/flexibee-bricks": "^1",
        "listonoska/api": "^1.1",
        "spipu/html2pdf": "*", 
        "tecnickcom/tcpdf": "6.2.21" 
    },
    "autoload": {
        "psr-4": {
            "FlexiPeeHP\\Reminder\\": "src/FlexiPeeHP/Reminder/",
            "FlexiPeeHP\\Reminder\\Notifier\\": "src/FlexiPeeHP/Reminder/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\FlexiPeeHP\\": "vendor/spoje.net/flexibee/testing/src/FlexiPeeHP/",
            "Test\\FlexiPeeHP\\Reminder\\": "tests/FlexiPeeHP/Reminder/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    }
}
